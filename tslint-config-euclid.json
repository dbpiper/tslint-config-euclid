{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules-recommended",
    "tslint-config-airbnb",
    "tslint-config-airbnb-base"
  ],
  "jsRules": {},
  "rules": {
    "align": [
      true,
      "statements",
      "members"
    ],
    "import-name": false,
    "interface-name": false,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-console": {
      "severity": "warning"
    },
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-unused-variable": false,
    "object-literal-sort-keys": false,
    "prefer-conditional-expression": false,
    "quotemark": [
      true,
      "single",
      "avoid-templates",
      "jsx-double"
    ],
    "ter-arrow-body-style": [
      true,
      "as-needed"
    ],
    "ter-arrow-parens": [
      true,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "ter-arrow-spacing": [
      true,
      {
        "after": true,
        "before": true
      }
    ],
    "ter-prefer-arrow-callback": [
      true,
      {
        "allowUnboundThis": false
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
