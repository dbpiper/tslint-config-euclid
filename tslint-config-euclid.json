{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules-recommended",
    "tslint-config-airbnb",
    "tslint-config-airbnb-base"
  ],
  "jsRules": {},
  "rules": {
    "align": [
      true,
      "statements",
      "members"
    ],
    "import-name": false,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-unused-variable": false,
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-templates",
      "jsx-double"
    ]
  }
}